source 'http://rubygems.org'

gem 'rails', '~> 5.1.0'

platforms :ruby, :mswin, :mingw do
  gem 'sqlite3'
end

platforms :jruby do
  gem 'jdbc-sqlite3'
  gem 'activerecord-jdbcsqlite3-adapter'
  gem 'mime-types', '~> 2.99.3'
  gem 'rake'
end

platforms :rbx do
  gem 'rubysl', '~> 2.0'
  gem 'minitest'
end

gemspec path: '../'
